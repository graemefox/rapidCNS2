// config file for rapid_cns2 nextflow

manifest.defaultBranch = 'main'

process.container = 'graefox/rapid_cns2:latest'
docker {
    enabled = 'true'
    docker.runOptions = '--cpus 64'
}

report {
    enabled = true
    overwrite = true
}

params.mgmt_pred = 'bin/mgmt_pred_v0.1.R'
params.probes = 'bin/mgmt_probes.Rdata'
params.mgmt_bed = 'bin/mgmt_hg38.bed'
params.model = 'bin/mgmt_137sites_mean_model.Rdata'
params.meth_class = 'bin/methylation_classification_nanodx_v0.1.R'
params.topprobes = 'bin/top_probes_hm450.Rdata'
params.trainingdata = 'bin/capper_top_100k_betas_binarised.Rdata'
params.arrayfile = 'bin/HM450.hg38.manifest.gencode.v22.Rdata'
params.threads = 64
params.filterreport = 'bin/filter_report_v0.1.R'
params.makereport = 'bin/make_report_v0.1.R'
params.report_UKHD = 'bin/Rapid_CNS2_report_UKHD_v0.1.Rmd'